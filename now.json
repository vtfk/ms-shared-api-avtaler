{
  "version": 2,
  "name": "ms-shared-api-avtaler",
  "scope": "telemark",
  "alias": [
    "avtaler.api.service.t-fk.no"
  ],
  "env": {
    "NODE_ENV": "production",
    "JWT_SECRET": "@tfk-jwt-secret",
    "MONGODB_CONNECTION": "@db-tfkcosmostest-connection",
    "MONGODB_COLLECTION": "avtaler",
    "MONGODB_NAME": "felles",
    "SVARUT_SERVICE_URL": "url-for-your-svarut-service",
    "SVARUT_SERVICE_SECRET": "jwt-secret-for-your-svarut-service",
    "PAPERTRAIL_HOST": "@tfk-papertrail-host",
    "PAPERTRAIL_PORT": "@tfk-papertrail-port",
    "PAPERTRAIL_HOSTNAME": "elevpc"
  },
  "builds": [
    { 
      "src": "/index.js",
      "use": "@now/node"
    },
    { 
      "src": "/*.md",
      "use": "@now/md"
    }
  ],
  "routes": [
    { 
      "src": "/",
      "dest": "/README.html"
    },
    { 
      "src": "/docs",
      "dest": "/README.html"
    },
    { 
      "src": "/agreements/(.*)",
      "dest": "/index.js"
    }
  ]
}